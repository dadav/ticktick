# TickTick Configuration

# Database path (default: ./data/ticktick.db)
TICKTICK_DB_PATH=./data/ticktick.db

# Weekly work hours requirement (default: 41)
TICKTICK_WEEKLY_HOURS=41

# Maximum daily work hours (default: 10)
TICKTICK_MAX_DAILY_HOURS=10

# Hours before lunch break is deducted (default: 6)
TICKTICK_LUNCH_THRESHOLD=6

# Lunch break duration in minutes (default: 30)
TICKTICK_LUNCH_DURATION=30

# Server host (default: 0.0.0.0)
TICKTICK_HOST=0.0.0.0

# Server port (default: 8000)
TICKTICK_PORT=8000
